{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://www.facebook.com/*"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "My Cool Extension",
  "permissions": [
    "http://*/",
    "https://*/",
    "power",
    "idle",
    "contextMenus"
  ],
  "version": "0.1"
}